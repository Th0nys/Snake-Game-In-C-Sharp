<Application x:Class="Snake2._0.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Snake2._0"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundColor">#003366</SolidColorBrush>
        <SolidColorBrush x:Key="GridBackgorundColor">#0059b3</SolidColorBrush>
        <SolidColorBrush x:Key="GridLineColor">#00cc66</SolidColorBrush>
        <SolidColorBrush x:Key="TextColor">#FFFFFF</SolidColorBrush>
        <SolidColorBrush x:Key="OverlayColor">#7F000000</SolidColorBrush>
        <FontFamily x:Key="MainFont">Assets/#Droid Sans Mono</FontFamily>
    </Application.Resources>
</Application>
